{
  "name": "marko-amp",
  "version": "0.1.0",
  "description": "Marko taglib for Accelerated Mobile Pages custom tags",
  "main": "index.js",
  "scripts": {
    "test": "jest && eslint *.js --ignore-path .gitignore && remark . --quiet --frail",
    "release": "npmpub"
  },
  "engines": {
    "node": "^8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/marko-amp.git"
  },
  "peerDependencies": {
    "marko": "^4"
  },
  "keywords": [
    "markojs",
    "amphtml",
    "amp",
    "marko",
    "taglib",
    "html",
    "template",
    "templating",
    "custom",
    "tags"
  ],
  "author": "tinovyatkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/marko-amp/issues"
  },
  "homepage": "https://github.com/tinovyatkin/marko-amp#readme",
  "devDependencies": {
    "@destinationstransfers/eslint-config": "^1.0.2",
    "eslint": "^4.6.1",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-node": "^5.1.1",
    "jest": "^21.0.1",
    "marko": "^4.4.28",
    "npmpub": "^3.1.0",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.1",
    "remark-preset-lint-recommended": "^3.0.1"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "@destinationstransfers",
      "plugin:node/recommended"
    ],
    "env": {
      "shared-node-browser": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prefer-object-spread/prefer-object-spread": "off"
    }
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  },
  "dependencies": {
    "common-tags": "^1.4.0"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testMatch": [
      "**/*.test.js"
    ]
  }
}
