{
  "name": "marko-amp",
  "version": "0.4.0",
  "description": "Marko taglib for Accelerated Mobile Pages custom tags",
  "main": "index.js",
  "scripts": {
    "test": "jest && eslint \"**/*.js\" --ignore-path .gitignore && remark . --quiet --frail",
    "release": "npmpub"
  },
  "engines": {
    "node": ">=8.6"
  },
  "files": [
    "components",
    "marko.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/marko-amp.git"
  },
  "peerDependencies": {
    "marko": "^4"
  },
  "keywords": [
    "markojs",
    "amphtml",
    "amp",
    "marko",
    "taglib",
    "html",
    "template",
    "templating",
    "custom",
    "tags"
  ],
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/marko-amp/issues"
  },
  "homepage": "https://github.com/tinovyatkin/marko-amp#readme",
  "devDependencies": {
    "eslint": "^4.13.1",
    "jest": "^21.2.1",
    "marko": "^4.7.3",
    "npmpub": "^3.1.0",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.1",
    "remark-preset-lint-recommended": "^3.0.1",
    "@destinationstransfers/eslint-config": "^1.7.0"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "@destinationstransfers",
      "plugin:node/recommended"
    ],
    "env": {
      "shared-node-browser": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  },
  "dependencies": {
    "common-tags": "^1.5.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testMatch": [
      "**/*.test.js"
    ]
  }
}
